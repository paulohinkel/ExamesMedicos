@{
    ViewBag.Title = "Cadastrar";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Cadastrar Examinador</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>



<div class="panel panel-default">
    <div class="panel-heading">
        Cadastro
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-lg-12">
                <form action="~/Examinador/Cadastrar" autocomplete="on" method="post" role="form">
                    <div class="form-group">
                        <label>Nome</label>
                        <input type="text" id="Nome" name="Nome" required class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Email</label>
                        <input type="email" id="Email" name="Email" required class="form-control">
                    </div>

                    <div class="form-group">
                        <label>Cremers</label>
                        <input type="number" id="Cremers" name="Cremers" required class="form-control">
                    </div>

                    <div class="form-group">
                        <label>Especialidade</label>
                        <input type="text" id="Especialidade" name="Especialidade" required class="form-control">
                    </div>

                    <div class="form-group">
                        <label>Telefone</label>
                        <input type="number" id="Telefone" name="Telefone" required class="form-control">
                    </div>
                    <div class="form-group">
                        <input type="submit" id="btCadastrar" name="btCadastrar" value="Cadastrar" class="btn btn-primary" />
                    </div>
                </form>
            </div>

        </div>
        <!-- /.row (nested) -->
    </div>
    <!-- /.panel-body -->
</div>

<p>
Nome: @ViewBag.Nome <br/>
Email: @ViewBag.Email <br /> 
Cremers: @ViewBag.Cremers <br/>
Especialidade: @ViewBag.Especialidade <br/>
Telefone: @ViewBag.Telefone 
</p>
