@{
    ViewBag.Title = "Cadastrar";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Cadastrar Exame</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>



<div class="panel panel-default">
    <div class="panel-heading">
        Cadastro
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-lg-12">
                <form action="~/Exame/Cadastrar" autocomplete="on" method="post" role="form">
                    <div class="form-group">
                        <label>Tipo</label>
                        <select name="tipo" id="tipo" class="form-control">
                            <option>Exame 1</option>
                            <option>Exame 2</option>
                            <option>Exame 3</option>
                            <option>Exame 4</option>
                        </select>
                        
                    </div>
                    <div class="form-group">
                        <label>Examinador</label>
                        <select name="examinador" id="examinador" class="form-control">
                            <option>Examinador 1</option>
                            <option>Examinador 2</option>
                            <option>Examinador 3</option>
                            <option>Examinador 4</option>
                        </select>
                    </div>

          
                    <div class="form-group">
                        <input type="submit" id="btCadastrar" name="btCadastrar" value="Cadastrar" class="btn btn-primary" />
                    </div>
                </form>
            </div>

        </div>
        <!-- /.row (nested) -->
    </div>
    <!-- /.panel-body -->
</div>

<p>
Tipo: @ViewBag.Tipo <br/>
Examinador: @ViewBag.Examinador <br /> 
</p>

